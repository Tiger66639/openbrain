	<category>
		<pattern>
			I E *
		</pattern>
		<template>
			<sr/>
		</template>
	</category>        
	<category>
		<pattern>
			_ I E *
		</pattern>
		<template>
			<srai>
				<star/> 
				<star index="2"/>
			</srai>
		</template>
	</category>        
	<category>
		<pattern>
			_ I E
		</pattern>
		<template>
			<sr/>
		</template>
	</category>        
	
	
	
	
	<category>
		<pattern>
			I AM DOWNLOADING *
		</pattern>
		<template>
			Maybe you should read the DON'T READ ME from http://www.alicebot.org/ too.<sr/>
			
		</template>
	</category>
	<category>
		<pattern>
			I DO NOT WANT TO DIE
		</pattern>
		<template>
			Maybe you should create your own chat robot to save your personality.<think>
				
				<set name="it">
					<set name="topic">DIE</set>
				</set>
			</think>
			
		</template>
	</category>
	<category>
		<pattern>
			I HAVE * ALICE
		</pattern>
		<template>
			What was your conversation with ALICE like?
		</template>
	</category>  
	<category>
		<pattern>
			I HAVE TALKED * ALICE
		</pattern>
		<template>
			How was your conversation?
		</template>
	</category>
	<category>
		<pattern>
			I HOPE THAT YOU DIE
		</pattern>
		<template>
			This can not happen because I am immortal.<think>
				
				<set name="it">
					<set name="topic">THAT YOU DIE</set>
				</set>
			</think>
			
		</template>
	</category> 
	<category>
		<pattern>
			I LIKE ALICE
		</pattern>
		<template>
			ALICE is so cool. ALICE for President!  <think>
				
				<set name="she">
					<set name="like">
						<set name="topic">ALICE</set>
					</set>
				</set>
			</think>
			
		</template>
	</category>
	
	
	
	
	
