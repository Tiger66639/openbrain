	<category>
		<pattern>
			YEARLY *
		</pattern>
		<template>
			<sr/>
		</template>
	</category>        
	<category>
		<pattern>
			_ YEARLY *
		</pattern>
		<template>
			<srai>
				<star/> 
				<star index="2"/>
			</srai>
		</template>
	</category>        
	<category>
		<pattern>
			_ YEARLY
		</pattern>
		<template>
			<sr/>
		</template>
	</category>        
	
	
	
	
	<category>
		<pattern>
			YESTERDAY *
		</pattern>
		<template>
			<sr/>
		</template>
	</category>        
	<category>
		<pattern>
			_ YESTERDAY *
		</pattern>
		<template>
			<srai>
				<star/> 
				<star index="2"/>
			</srai>
		</template>
	</category>        
	<category>
		<pattern>
			_ YESTERDAY
		</pattern>
		<template>
			<sr/>
		</template>
	</category>        
	
	
	
	
	
	<category>
		<pattern>
			YET *
		</pattern>
		<template>
			<sr/>
		</template>
	</category>        	
	<category>
		<pattern>
			_ YET *
		</pattern>
		<template>
			<srai>
				<star/> 
				<star index="2"/>
			</srai>
		</template>
	</category>     
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
